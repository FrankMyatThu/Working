  <system.serviceModel>

    <serviceHostingEnvironment aspNetCompatibilityEnabled="true" multipleSiteBindingsEnabled="true"/>

    <behaviors>
      <serviceBehaviors>        
        <behavior name="serviceBehavior">
          <dataContractSerializer maxItemsInObjectGraph="2147483647"/>          
          <serviceDebug includeExceptionDetailInFaults="true" />
          <serviceMetadata httpGetEnabled="true"  />          
        </behavior>
      </serviceBehaviors>
      <endpointBehaviors>
      </endpointBehaviors>
    </behaviors>

    <bindings>
      <wsHttpBinding>
        <binding name="httpLargeMessageStream"
                  bypassProxyOnLocal="true"
                  maxReceivedMessageSize="2147483647"
                  sendTimeout="00:25:00"
                  maxBufferPoolSize="2147483647"
                  messageEncoding="Mtom" >
			<security mode="None" />
			<readerQuotas
                maxDepth="2147483647"
                maxStringContentLength="2147483647"
                maxArrayLength="2147483647"
                maxBytesPerRead="2147483647"
                maxNameTableCharCount="2147483647" />

        </binding>
      </wsHttpBinding>      
    </bindings>

	<services>
		<service name="RMP.OTAService.FileService" behaviorConfiguration="serviceBehavior">        
			<endpoint address=""
			  binding="wsHttpBinding" bindingConfiguration="httpLargeMessageStream"
			  contract="RMP.OTAService.IFileService"
			  name="FileServiceEndPoint">
			</endpoint>
			<endpoint address="mex"
			  binding="mexHttpBinding"
			  contract="IMetadataExchange" >
			</endpoint>        
		</service>
		<service name="RMP.OTAService.TokenService" behaviorConfiguration="serviceBehavior">
			<endpoint address=""
				binding="wsHttpBinding" bindingConfiguration="httpLargeMessageStream" 
				contract="RMP.OTAService.ITokenService"
				name="TokenServiceEndPoint">
			</endpoint>
			<endpoint address="mex"
				binding="mexHttpBinding"
				contract="IMetadataExchange"/>
		</service>	  
	</services>

  </system.serviceModel> 
